def gv
pipeline{
    agent any
    
    stages{ 

        stage("init"){
            gv= load "file.groovy"
        }

        stage("build"){
            steps{
                script{
                    gv.build()
                }
            }
        }
        stage("test"){
            steps{
                script{
                    gv.test()
                }
            }
        }
        stage("deploy"){
            steps{
                script{
                    gv.deploy()
                }
            }
        }
    }
}