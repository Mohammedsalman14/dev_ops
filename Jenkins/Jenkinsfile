pipeline {
    agent any

    stages {
        stage("init"){
            steps{
                script{
                    def gv= load 'file.groovy'
                }
            }
        }
        stage('Build') {
            steps {
                echo "building the app"
            }
        }
        
        stage('Test') {
            steps {
               echo "testing the app"
            }
        }
        stage('Deploy') {
            steps {
                echo "deploying the app"
            }
        }
    }
}
