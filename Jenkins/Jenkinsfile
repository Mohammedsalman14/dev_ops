def myFile = load 'file.groovy'
pipeline{
    agent any
    stages{
        stage("build"){
            steps{
                script{
                    myFile.build()
                }
            }
        }
        stage("test"){
            steps{
                script{
                    myFile.test()
                }
            }
        }
        stage("deploy"){
            steps{
                script{
                    myFile.deploy()
                }
            }
        }
    }
}