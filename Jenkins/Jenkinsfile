def myFile
pipeline{
    agent any
    stages{
        stage("init"){
            steps{
                script{
                    myFile= = load "./file.groovy"
                }
            }     
        }


        stage("build"){
            steps{
                script{
                    myFile.build()
                }
            }
        }
        stage("test"){
            steps{
                script{
                    myFile.test()
                }
            }
        }
        stage("deploy"){
            steps{
                script{
                    myFile.deploy()
                }
            }
        }
    }
}