pipeline {
    agent any

    stages {
        stage('Load Script') {
            steps {
                script {
                    gv = load 'file.groovy'
                }
            }
        }

        stage("Build") {
            steps {
                script {
                    gv.build()
                }
            }
        }

        stage("Test") {
            steps {
                script {
                    gv.test()
                }
            }
        }

        stage("Deploy") {
            steps {
                script {
                    gv.deploy()
                }
            }
        }
    }
}
