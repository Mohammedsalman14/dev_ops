def gv= load 'file.groovy'
pipeline{
    agent any
    stages{

        stage("build"){
            steps{
                script{
                    gv.build()
                }
            }
        }
        stage("test"){
            steps{
                script{
                    gv.test()
                }
            }
        }
        stage("deploy"){
            steps{
                script{
                    gv.deploy()
                }
            }
        }
    }
}